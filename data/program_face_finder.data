/value><next><block type=\"variables_set\" id=\"159\" inline=\"true\"><field name=\"VAR\">face_size</field><value name=\"VALUE\"><block type=\"lists_getIndex\" id=\"160\" inline=\"true\"><mutation statement=\"false\" at=\"true\"></mutation><field name=\"MODE\">GET</field><field name=\"WHERE\">FROM_START</field><value name=\"VALUE\"><block type=\"variables_get\" id=\"161\"><field name=\"VAR\">face_x_y_size</field></block></value><value name=\"AT\"><block type=\"math_number\" id=\"162\"><field name=\"NUM\">3</field></block></value></block></value><next><block type=\"text_print\" id=\"163\" inline=\"false\"><value name=\"TEXT\"><block type=\"text_join\" id=\"164\" inline=\"false\"><mutation items=\"7\"></mutation><value name=\"ADD0\"><block type=\"text\" id=\"165\"><field name=\"TEXT\">face at </field></block></value><value name=\"ADD1\"><block type=\"variables_get\" id=\"166\"><field name=\"VAR\">face_x</field></block></value><value name=\"ADD2\"><block type=\"text\" id=\"167\"><field name=\"TEXT\">x, </field></block></value><value name=\"ADD3\"><block type=\"variables_get\" id=\"168\"><field name=\"VAR\">face_y</field></block></value><value name=\"ADD4\"><block type=\"text\" id=\"169\"><field name=\"TEXT\">y, </field></block></value><value name=\"ADD5\"><block type=\"variables_get\" id=\"170\"><field name=\"VAR\">face_size</field></block></value><value name=\"ADD6\"><block type=\"text\" id=\"171\"><field name=\"TEXT\"> size</field></block></value></block></value><next><block type=\"controls_if\" id=\"172\" inline=\"false\"><mutation elseif=\"1\"></mutation><value name=\"IF0\"><block type=\"logic_compare\" id=\"173\" inline=\"true\"><field name=\"OP\">GT</field><value name=\"A\"><block type=\"variables_get\" id=\"174\"><field name=\"VAR\">face_x</field></block></value><value name=\"B\"><block type=\"math_number\" id=\"175\"><field name=\"NUM\">10</field></block></value></block></value><statement name=\"DO0\"><block type=\"coderbot_adv_move\" id=\"176\" inline=\"true\"